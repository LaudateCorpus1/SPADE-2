spade_root = ./
# All paths relative to spade_root unless absolute
local_control_port = 19999
commandline_query_port = 19998
remote_sketch_port = 29998
connection_timeout = 15000
source_reporter = source_reporter
logger_level = ALL
temp_directory = tmp
log_directory = log
# Expects SimpleDateFormat format
log_filename_pattern = 'SPADE_'MM.dd.yyyy-H.mm.ss'.log'
database_directory = db
config_directory = cfg
library_directory = lib
query_history_file = cfg/query.history
control_history_file = cfg/control.history
spade_host_file = spade.host
spade_pid_file = spade.pid
server_public_keystore = cfg/keys/keystore/serverpublic.keystore
server_private_keystore = cfg/keys/keystore/serverprivate.keystore
client_public_keystore = cfg/keys/keystore/clientpublic.keystore
client_private_keystore = cfg/keys/keystore/clientprivate.keystore
password_public_keystore = public_password
password_private_keystore = private_password
